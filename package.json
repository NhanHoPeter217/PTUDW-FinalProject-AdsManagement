{
    "name": "adsmanagement-hcmus-webdev",
    "version": "1.0.0",
    "description": "",
    "workspaces": [
        "CANBO",
        "NGUOIDAN"
    ],
    "scripts": {
        "start": "node app.js",
        "dev": "concurrently \"npm:dev-CANBO\" \"npm:dev-NGUOIDAN\"",
        "dev-CANBO": "cd ./CANBO && npm run dev",
        "dev-NGUOIDAN": "cd ./NGUOIDAN && npm run dev",
        "pretty": "prettier --check .",
        "pretty-fix": "prettier --write .",
        "prepare": "husky install",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*": [
          "prettier --write",
          "git add"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-fileupload": "^1.4.3",
        "express-handlebars": "^7.1.2",
        "express-rate-limit": "^5.3.0"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.1.0"
    }
}
